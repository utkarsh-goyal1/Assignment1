import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int sum=0;
      //  int a=9;
        while(true){
                int n=sc.nextInt();
                sum=sum+n;
                if (sum>=0){
                    System.out.println(n);
                }
                if (sum<0)
                {
                    break;
                }

        }
		
		

    }
}
