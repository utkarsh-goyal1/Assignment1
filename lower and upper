import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        char c=sc.next().charAt(0);
        if(c>='A' && c<='Z'){
            System.out.print("UPPERCASE");
        }
        else if(c>='a' && c<='z')
        {
            System.out.print("lowercase");
        }
        else{
            System.out.print("Invalid");
        }
    }
}
