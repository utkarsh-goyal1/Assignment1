import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        for(int i=0;i<t;i++)
        {
            int n=sc.nextInt();
            int b=0;
            int p=1;
            while(n>0)
            {
                int r=n%10;
                b=b+r*p;
                p=p*2;
                n=n/10;

            }
            System.out.println(b);
            b=0;
        }
    }
}
